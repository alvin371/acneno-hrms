  "private": true,
  "scripts": {
    "check-node": "node -e \"const [major, minor] = process.versions.node.split('.').map(Number); if (major < 20 || (major === 20 && minor < 12)) { console.error('\\n\\x1b[31m✖ Node.js version too old!\\x1b[0m\\n\\nYou are using Node.js v' + process.versions.node + ', but this project requires Node.js >= v20.12.0.\\nThe util.styleText function used by React Native CLI was added in v20.12.0.\\n\\n\\x1b[33mTo fix this, upgrade Node.js:\\x1b[0m\\n  • nvm: nvm install 20.12.0 && nvm use 20.12.0\\n  • brew: brew upgrade node\\n  • n: n 20.12.0\\n'); process.exit(1); }\"",
    "start": "pnpm check-node && react-native start",
    "android": "pnpm check-node && react-native run-android",
    "ios": "pnpm check-node && react-native run-ios",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "ios:archive": "xcodebuild -workspace ios/HrManagementSystem.xcworkspace -scheme HrManagementSystem -configuration Release -archivePath \"$HOME/Library/Developer/Xcode/Archives/$(date +%Y-%m-%d)/HrManagementSystem $(date +%Y-%m-%d\\ %H.%M.%S).xcarchive\" archive"
  },
